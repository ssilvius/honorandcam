---
import { getEntry } from 'astro:content';
import Layout from '../layouts/Layout.astro';

const homeContent = await getEntry('pages', 'home');
let Content;
if (homeContent) {
  ({ Content } = await homeContent.render());
} else {
  Content = () => null; // Fallback component
}
---

<Layout title="Honor & Cam's Wedding">
  <Content />
</Layout>
